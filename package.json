{
  "name": "domtastic",
  "version": "0.12.3",
  "description": "Small, fast, and modular DOM and event library for modern browsers.",
  "keywords": [
    "DOM",
    "attr",
    "class",
    "domtastic",
    "es6",
    "event",
    "evergreen",
    "html",
    "jquery",
    "selector"
  ],
  "author": {
    "email": "lars@webpro.nl",
    "name": "Lars Kappert",
    "url": "https://webpro.nl"
  },
  "homepage": "https://domtastic.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/webpro/DOMtastic.git"
  },
  "bugs": {
    "url": "https://github.com/webpro/DOMtastic/issues"
  },
  "main": "dist/domtastic.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "src",
    "src-jquery-compat"
  ],
  "scripts": {
    "benchmark": "npm run build && benchrunner benchmark/polyfill.bind.js benchmark/init.js benchmark/class.js benchmark/construct.js benchmark/dom.js benchmark/selector.js",
    "build": "rollup -c && rollup -c --minify",
    "bundle": "rollup -c",
    "coverage": "nyc --reporter text --reporter html npm test",
    "doc": "doxstrap --title DOMtastic --output doc",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "prepublish": "npm run build",
    "test": "mocha",
    "test:jquery": "npm run bundle -- --jquery-compat && npm test",
    "test:bs": "browserstack-runner",
    "vendor": "build/vendor.sh",
    "watch": "onchange 'src/**/*.js' -- npm run bundle"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.23.0",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.23.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.22.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "benchrunner": "0.0.4",
    "browserstack-runner": "^0.5.0",
    "chai": "^3.5.0",
    "classlist-polyfill": "^1.0.3",
    "doxstrap": "0.0.7",
    "eslint": "^3.17.0",
    "eslint-plugin-import": "^2.2.0",
    "glob": "7.1.1",
    "jquery": "^3.1.1",
    "jsdom": "^9.11.0",
    "jsdom-global": "^2.1.1",
    "lodash": "^4.17.4",
    "minimist": "1.2.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "onchange": "3.2.1",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "zepto": "^1.2.0"
  },
  "license": "MIT"
}
